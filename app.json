      "package": "com.andrew_react.UsefulList"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "eb46b16b-4f00-4c4c-ac74-730ea1fe6eae"
